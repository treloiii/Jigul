<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
    <configuration>
        <webPackages>
            <webPackage>com.trelloiii.jigul.scanner.test</webPackage>
        </webPackages>
        <iocPackages>
            <iocPackage>com.trelloiii.jigul.scanner.test</iocPackage>
        </iocPackages>
        <serverPort>8082</serverPort>
    </configuration>
    <beans>
        <bean>
            <classPath>com.trelloiii.jigul.scanner.test.Person</classPath>
            <constructorArgs>
                <constructorArg>"XML PERSON"</constructorArg>
                <constructorArg>"XML PERSON SURNAME"</constructorArg>
            </constructorArgs>
            <constructorTypes>
                <constructorType>java.lang.String</constructorType>
                <constructorType>java.lang.String</constructorType>
            </constructorTypes>
            <instanceType>Multi</instanceType>
        </bean>
    </beans>
</config>